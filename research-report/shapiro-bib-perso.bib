@InProceedings{syn:sh188,
  Keywords =     {IntConf},
  author =	 {Alejandro Z. Tomsic and Tyler Crain and Marc Shapiro},
  title =	 {Physi{CS}-{NMSI}: efficient consistent snapshots for
                  scalable snapshot isolation},
  booktitle =	 papoc,
